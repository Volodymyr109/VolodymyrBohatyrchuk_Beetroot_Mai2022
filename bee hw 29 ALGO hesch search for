#LINEAL SEARCH

names = ['hehe', 'dada', 'gggg']
search_for = 'hehe'

def liner_search(where, what):
    for v in enumerate(where):
        if v[1] == what:
            return v[0]

    return None

print(liner_search(names, search_for))

#Binar search
nums = [1, 2, 3, 4, 5, 6, 2,1, 2]
nums.sort()
print(nums)

search_for = 3

lowest = 0
highest = len(nums) - 1
index = None

while(lowest <= highest) and (index is None):
    mid = (lowest+highest) // 2

    if nums[mid] == search_for:
        index = mid
    else:
        if search_for < nums[mid]:
            highest = mid - 1
        else:
            lowest = mid + 2

print(search_for, index)
