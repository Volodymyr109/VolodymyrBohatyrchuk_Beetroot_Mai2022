#TASK_1
class Animal:

    def __init__(self, name):
        self.name = name

    def talk(self):
        raise NotImplementedError(' Must be implemented by a sub class')

class Cat(Animal):
    def talk(self):
        print('MEOW!')

class Dog(Animal):
    def talk(self):
        print('Woof!')

animals = [Cat('Keks'), Dog('Hanz'), Cat('Lolka')]

for animal in animals:
    animal.talk()
